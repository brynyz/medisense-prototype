# Production Environment Variables for React Frontend
# These will be used when building for production

# API Base URL - Update this with your DigitalOcean app URL
REACT_APP_API_URL=https://your-app-name.ondigitalocean.app/api

# App Configuration
REACT_APP_APP_NAME=MediSense
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# External Services (if needed)
# REACT_APP_MAPBOX_TOKEN=your-mapbox-token-here
# REACT_APP_GOOGLE_ANALYTICS_ID=your-ga-id-here

# Build Configuration
GENERATE_SOURCEMAP=false
BUILD_PATH=build
