# Development Environment Variables for React Frontend
# These will be used during local development

# API Base URL - Local Django development server
REACT_APP_API_URL=http://localhost:8000/api

# App Configuration
REACT_APP_APP_NAME=MediSense (Dev)
REACT_APP_VERSION=1.0.0-dev

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_NOTIFICATIONS=true

# Development Settings
REACT_APP_DEBUG=true
GENERATE_SOURCEMAP=true
